    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.11",
    "@types/webpack": "^5.28.0",
    "@webpack-cli/generators": "^2.4.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "node": "^17.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
