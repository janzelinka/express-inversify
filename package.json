{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --watch src/**/* --ext ts,json --ignore src/**/*.spec.ts --exec ts-node src/index.ts",
    "docker-start": "ts-node src/index.ts",
    "generate-migration": "npx ts-node --transpile-only node_modules/typeorm/cli.js migration:generate './src/database/migration/User' -d './src/database/dataSource/dataSource.ts'",
    "docker-container-start": "docker-compose up -d",
    "docker-container-build": "docker-compose build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
