{
  "name": "upgrade",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src/**/* --ext ts,json --ignore src/**/*.spec.ts --exec ts-node src/index.ts",
    "build": "npx tsc",
    "migrate:generate": "npx ts-node --transpile-only node_modules/typeorm/cli.js migration:generate './src/database/migration/User' -d './src/database/dataSource/dataSource.ts'",
    "migrate:run": "npx ts-node --transpile-only node_modules/typeorm/cli.js migration:run -d './src/database/dataSource/dataSource.ts'",
    "seed:run": "node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./src/database/dataSource/dataSource.ts",
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "devDependencies": {
    "@inversifyjs/json-schema-types": "^1.0.1",
    "@inversifyjs/open-api-types": "^1.0.1",
    "@types/express": "^4.0.0",
    "@types/node": "^24.10.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typeorm-extension": "^3.7.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@inversifyjs/http-core": "^4.3.0",
    "@inversifyjs/http-express-v4": "^4.3.0",
    "@inversifyjs/http-open-api": "^4.3.0",
    "express": "^4.21.1",
    "inversify": "^7.10.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "0.3.27"
  }
}
