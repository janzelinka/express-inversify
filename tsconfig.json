{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",
    "lib": ["es6", "dom"],
    "noImplicitAny": false,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noUnusedLocals": true
  },
  "compileOnSave": false,
  "exclude": ["node_modules"]
}
